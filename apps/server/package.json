{
  "name": "server",
  "version": "1.0.0",
  "description": "Fastify server",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jest",
    "start": "node dist/server.js",
    "build": "rimraf dist && tsc && tsc-alias",
    "watch": "concurrently \"tsc -w\" \"tsc-alias -w\"",
    "dev": "npm run build && concurrently -k -p \"[{name}]\" -n \"TypeScript,server\" -c \"yellow.bold,cyan.bold\" \"npm:watch\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/accepts": "^5.0.3",
    "@fastify/autoload": "^6.0.0",
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/routes": "^6.0.2",
    "@fastify/sensible": "^6.0.0",
    "@fastify/swagger": "^9.5.2",
    "@fastify/swagger-ui": "^5.2.3",
    "@fastify/websocket": "^11.2.0",
    "@joggr/fastify-prisma": "^6.0.1",
    "@prisma/client": "^6.13.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "better-auth": "^1.3.34",
    "dayjs": "^1.11.19",
    "dotenv": "^16.4.7",
    "fastify": "^5.0.0",
    "fastify-cli": "^7.4.0",
    "fastify-plugin": "^5.0.0",
    "ioredis": "^5.8.2",
    "pino": "^9.14.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^22.1.0",
    "@types/ws": "^8.18.1",
    "concurrently": "^9.0.0",
    "fastify-tsconfig": "^3.0.0",
    "jest": "^30.2.0",
    "prisma": "^6.18.0",
    "rimraf": "^4.4.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.4.0",
    "tsc-alias": "^1.8.16",
    "typescript": "~5.8.2"
  }
}
