{
  "name": "fn-boilerplate",
  "description": "Fullstack boilerplate using Fastify and Nuxt",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "devDependencies": {
    "husky": "^9.1.7",
    "lerna": "^9.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1"
  },
  "scripts": {
    "dev": "lerna run dev --parallel",
    "build": "lerna run build --parallel",
    "test": "lerna run test --parallel",
    "lint": "lerna run lint --parallel",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=24.11.0"
  }
}
